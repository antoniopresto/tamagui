    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.3.3",
    "vitest": "^0.34.3",
    "vitest-axe": "^0.1.0",
    "@types/react": "~18.2.14",
    "@types/react-dom": "^18.2.19"
  },
  "dependencies": {
    "@tamagui/checkbox": "1.90.2",
    "@tamagui/config-default": "1.90.2",
    "@tamagui/core": "1.90.2",
    "@tamagui/button": "1.90.2"
  }
