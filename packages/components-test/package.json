{
  "name": "@tamagui/components-test",
  "version": "1.90.6",
  "module": "dist/esm",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "true",
    "test:web": "DISABLE_REACT_NATIVE=1 TEST_ENVIRONMENT=jsdom TAMAGUI_TARGET=web vitest --run --config ../vite-plugin-internal/src/vite.config.ts **/*.web.test.tsx",
