{
  "name": "@tamagui/core-test",
  "version": "1.90.6",
  "module": "dist/esm",
  "private": true,
  "license": "MIT",
  "author": {
    "name": "Nate Wienert"
  },
  "scripts": {
    "build": "true",
    "test:run": "vitest --run --config ../vite-plugin-internal/src/vite.config.ts",
    "test:web": "TAMAGUI_TARGET=web yarn test:run **/*.web.test.tsx",
    "test:native": "TAMAGUI_TARGET=native yarn test:run **/*.native.test.tsx",
    "test": "yarn test:native && yarn test:web"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-native": "^12.4.3",
    "csstype": "^3.0.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.3.3",
    "vitest": "^0.34.3",
    "@types/react": "~18.2.14",
    "@types/react-dom": "^18.2.19"
  },
  "dependencies": {
    "@tamagui/core": "1.90.2",
    "@tamagui/config-default": "1.90.2"
  }
}
